{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "astro-portal/HeadPortal.astro": ["src/components/HeadPortal.astro"]
    },
    "rootDir": "src",
    "declaration": true,
    "emitDeclarationOnly": false,
    "strict": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "types": ["astro/astro-jsx"]
  },
  "include": ["src"]
}
